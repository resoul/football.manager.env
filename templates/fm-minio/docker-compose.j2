  fm-minio:
    image: minio/minio:latest
    command: server /data --console-address ":9001"
    ports:
      - "9000:9000"
      - "9001:9001"
    environment:
      MINIO_ROOT_USER: fm-minio
      MINIO_ROOT_PASSWORD: fm-minio
      DOMAINS: minio.fm.localhost
    volumes:
      - ./volumes/storage:/data:z